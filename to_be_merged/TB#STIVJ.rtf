\par \pard\plain \ltrpar\s1\ql\rtlch\afs24\lang255\ltrch\dbch\langfe1040\hich\fs20\lang1040\loch\fs20\lang1040 {\rtlch \ltrch\loch\f1\fs20\lang1040\i0\b0 END}
\par \pard\plain \ltrpar\s1\ql\rtlch\afs24\lang255\ltrch\dbch\langfe1040\hich\fs24\lang1040\loch\fs24\lang1040 
\par \pard\plain \ltrpar\s1\ql\rtlch\afs20\lang255\ltrch\dbch\langfe1040\hich\fs20\lang1040\loch\fs20\lang1040 {\rtlch \ltrch\loch\f1\fs20\lang1040\i0\b0 ANOMEN Sono molto preoccupato per ci\'f2 che \'e8 accaduto in questo luogo. Anche se la scelta sembra chiara e giusta, esito a compierla.}
\par \pard\plain \ltrpar\s1\ql\rtlch\afs24\lang255\ltrch\dbch\langfe1040\hich\fs24\lang1040\loch\fs24\lang1040{\rtlch \ltrch\loch\f1\fs24\lang1040\i0\b0{\rtlch\ltrch\hich\fs20\loch\fs20 Di certo, se Saerk ha ucciso mia sorella, devo vendicare il suo omicidio. Ma uccidere per vendicare \'e8 omicidio secondo i precetti dell'Ordine. }}{\rtlch \ltrch\loch\f1\fs24\lang1040\i0\b0{\rtlch\ltrch\hich\fs20\lang1033\loch\fs20\lang1033 Non so cosa fare.}}
\par \pard\plain \ltrpar\s1\ql\rtlch\afs20\lang255\ltrch\dbch\langfe1040\hich\fs20\lang1033\loch\fs20\lang1033 {\rtlch \ltrch\loch\f1\fs20\lang1033\i0\b0 INTERJEC\-\-T_COPY_TRANS ANOMENJ}
\par \pard\plain \ltrpar\s1\ql\rtlch\afs24\lang255\ltrch\dbch\langfe1040\hich\fs20\lang1033\loch\fs20\lang1033 {\rtlch \ltrch\loch\f1\fs20\lang1033\i0\b0 == TB#STIVJ IF ~IsValidForPartyDialog("Stivan") !StateCheck("Stivan",CD_STATE_NOTVALID)~ THEN}
\par \pard\plain \ltrpar\s1\ql\rtlch\afs20\lang255\ltrch\dbch\langfe1040\hich\fs20\lang1040\loch\fs20\lang1040 {\rtlch \ltrch\loch\f1\fs20\lang1040\i0\b0 ~Lo dico io cosa dobbiamo fare! E' giunto il momento che quell'infame paghi per ci\'f2 che ha fatto! Se non interveniamo *adesso*, un domani altri subiranno ci\'f2 che \'e8 successo a me e ad Anomen!~}
\par \pard\plain \ltrpar\s1\ql\rtlch\afs24\lang255\ltrch\dbch\langfe1040\hich\fs20\lang1040\loch\fs20\lang1040 {\rtlch \ltrch\loch\f1\fs20\lang1040\i0\b0 END}
\par \pard\plain \ltrpar\s1\ql\rtlch\afs20\lang255\ltrch\dbch\langfe1040\hich\fs20\lang1040\loch\fs20\lang1040 {\rtlch \ltrch\loch\f1\fs20\lang1040\i0\b0 IF ~Global(tb#StivanPCTalk","GLOBAL",2)~ THEN REPLY ~Mi \'e8 parso di capire che questo Saerk sia l'uomo che ti ha privato di tutti i tuoi beni, giusto?~ EXTERN TB#STIVJ SaerkStrikesAgain1-1}
\par \pard\plain \ltrpar\s1\ql\rtlch\afs24\lang255\ltrch\dbch\langfe1040\hich\fs20\lang1040\loch\fs20\lang1040 {\rtlch \ltrch\loch\f1\fs20\lang1040\i0\b0 IF ~!Global(tb#StivanPCTalk","GLOBAL",2)~ THEN REPLY ~Ho bisogno che tu scenda nei dettagli, Stivan. Chi \'e8 questo Saerk? Che cosa ti ha fatto?~ EXTERN TB#STIVJ SaerkStrikesAgain1-2}
\par \pard\plain \ltrpar\s1\ql\rtlch\afs24\lang255\ltrch\dbch\langfe1040\hich\fs24\lang1040\loch\fs24\lang1040 
\par \pard\plain \ltrpar\s1\ql\rtlch\afs20\lang255\ltrch\dbch\langfe1040\hich\fs20\lang1033\loch\fs20\lang1033 {\rtlch \ltrch\loch\f1\fs20\lang1033\i0\b0 APPEND TB#STIVJ}
\par \pard\plain \ltrpar\s1\ql\rtlch\afs24\lang255\ltrch\dbch\langfe1040\hich\fs20\lang1033\loch\fs20\lang1033 {\rtlch \ltrch\loch\f1\fs20\lang1033\i0\b0 IF THEN BEGIN SaerkStrikesAgain1-1}
\par \pard\plain \ltrpar\s1\ql\rtlch\afs20\lang255\ltrch\dbch\langfe1040\hich\fs20\lang1033\loch\fs20\lang1033 {\rtlch \ltrch\loch\f1\fs20\lang1033\i0\b0 SAY ~S\'ec, \'e8 lui.~ EXTERN TB#STIVJ SaerkStrikesAgain1-3}
\par \pard\plain \ltrpar\s1\ql\rtlch\afs24\lang255\ltrch\dbch\langfe1040\hich\fs20\lang1033\loch\fs20\lang1033 {\rtlch \ltrch\loch\f1\fs20\lang1033\i0\b0 END}
\par \pard\plain \ltrpar\s1\ql\rtlch\afs24\lang255\ltrch\dbch\langfe1040\hich\fs24\lang1040\loch\fs24\lang1040 
\par \pard\plain \ltrpar\s1\ql\rtlch\afs20\lang255\ltrch\dbch\langfe1040\hich\fs20\lang1033\loch\fs20\lang1033 {\rtlch \ltrch\loch\f1\fs20\lang1033\i0\b0 IF THEN BEGIN SaerkStrikesAgain1-2}
\par \pard\plain \ltrpar\s1\ql\rtlch\afs24\lang255\ltrch\dbch\langfe1040\hich\fs24\lang1040\loch\fs24\lang1040{\rtlch \ltrch\loch\f1\fs24\lang1040\i0\b0{\rtlch\ltrch\hich\fs20\lang1033\loch\fs20\lang1033 SAY ~Saerk \'e8 a capo dei Farrahd, una delle pi\'f9 ricche famiglie di Athkatla. Lui... era }}{\rtlch \ltrch\loch\f1\fs24\lang1040\i0\b0{\rtlch\ltrch\hich\fs20\loch\fs20 il socio in affari di mio padre.~}}
\par \pard\plain \ltrpar\s1\ql\rtlch\afs20\lang255\ltrch\dbch\langfe1040\hich\fs20\lang1040\loch\fs20\lang1040 {\rtlch \ltrch\loch\f1\fs20\lang1040\i0\b0 = ~Quando i miei genitori e i miei fratelli vennero uccisi dai gabbiani, gli chiesi subito aiuto. Non avevo la bench\'e8 minima idea di come gestire la mia eredit\'e0, ma pensavo che lui pi\'f9 di tutti mi avrebbe potuto dare una mano.~}
\par \pard\plain \ltrpar\s1\ql\rtlch\afs24\lang255\ltrch\dbch\langfe1040\hich\fs20\lang1040\loch\fs20\lang1040 {\rtlch \ltrch\loch\f1\fs20\lang1040\i0\b0 = ~Fu un errore fatale. Quel maledetto riusc\'ec ad impossessarsi degli atti di propriet\'e0 della mia famiglia, e fece realizzare dei falsi secondo cui le mie terre appartenevano in realt\'e0 ai Farrahd da generazioni.~}
\par \pard\plain \ltrpar\s1\ql\rtlch\afs24\lang255\ltrch\dbch\langfe1040\hich\fs24\lang1040\loch\fs24\lang1040{\rtlch \ltrch\loch\f1\fs24\lang1040\i0\b0{\rtlch\ltrch\hich\fs20\loch\fs20 = ~Persi tutto. I vigneti, la fattoria. E infine, anche la casa.~ }}{\rtlch \ltrch\loch\f1\fs24\lang1040\i0\b0{\rtlch\ltrch\hich\fs20\lang1033\loch\fs20\lang1033 EXTERN TB#STIVJ SaerkStrikesAgain1-3}}
\par \pard\plain \ltrpar\s1\ql\rtlch\afs20\lang255\ltrch\dbch\langfe1040\hich\fs20\lang1033\loch\fs20\lang1033 {\rtlch \ltrch\loch\f1\fs20\lang1033\i0\b0 END}
\par \pard\plain \ltrpar\s1\ql\rtlch\afs24\lang255\ltrch\dbch\langfe1040\hich\fs24\lang1040\loch\fs24\lang1040 
\par \pard\plain \ltrpar\s1\ql\rtlch\afs20\lang255\ltrch\dbch\langfe1040\hich\fs20\lang1033\loch\fs20\lang1033 {\rtlch \ltrch\loch\f1\fs20\lang1033\i0\b0 IF THEN BEGIN SaerkStrikesAgain1-3}
\par \pard\plain \ltrpar\s1\ql\rtlch\afs24\lang255\ltrch\dbch\langfe1040\hich\fs24\lang1040\loch\fs24\lang1040{\rtlch \ltrch\loch\f1\fs24\lang1040\i0\b0{\rtlch\ltrch\hich\fs20\lang1033\loch\fs20\lang1033 SAY ~*SOB*! }}{\rtlch \ltrch\loch\f1\fs24\lang1040\i0\b0{\rtlch\ltrch\hich\fs20\loch\fs20 Credevo di essermi lasciato alle spalle il mio burrascoso passato, ma sapere che Saerk continua a rovinare le vite altrui, esattamente come ha fatto con me... Mi fa impazzire dalla rabbia!~ }}
\par \pard\plain \ltrpar\s1\ql\rtlch\afs20\lang255\ltrch\dbch\langfe1040\hich\fs20\lang1040\loch\fs20\lang1040 {\rtlch \ltrch\loch\f1\fs20\lang1040\i0\b0 == MINSCJ IF ~IsValidForPartyDialog("Minsc") !StateCheck("Minsc",CD_STATE_NOTVALID)~ THEN ~Boo dice che chi \'e8 ingiusto ottiene sempre i migliori risultati.~}
\par \pard\plain \ltrpar\s1\ql\rtlch\afs24\lang255\ltrch\dbch\langfe1040\hich\fs20\lang1040\loch\fs20\lang1040 {\rtlch \ltrch\loch\f1\fs20\lang1040\i0\b0 = ~Boo! Come puoi dire una cosa simile? Tutti gli anni trascorsi a dar calci nelle chiappe al male... Spazzati via da una simile affermazione!~}
\par \pard\plain \ltrpar\s1\ql\rtlch\afs20\lang255\ltrch\dbch\langfe1040\hich\fs20\lang1040\loch\fs20\lang1040 {\rtlch \ltrch\loch\f1\fs20\lang1040\i0\b0 = ~Ohhhh! Capisco ora! Quell'uomo sta esercitando la sua malvagia influenza su di te! Dobbiamo fermarlo, prima che corrompa del tutto la bont\'e0 di questo criceto!~}
\par \pard\plain \ltrpar\s1\ql\rtlch\afs24\lang255\ltrch\dbch\langfe1040\hich\fs20\lang1040\loch\fs20\lang1040 {\rtlch \ltrch\loch\f1\fs20\lang1040\i0\b0 == EDWINJ IF ~IsValidForPartyDialog("Edwin") IsValidForPartyDialog("Minsc") !StateCheck("Edwin",CD_STATE_NOTVALID)~ THEN ~(Finalmente ho trovato il modo di impiegare quello stupido incantesimo chiamato "Dominare Animali". E pensare che stavo semplicemente 
cercando un valido passatempo per ignorare gli struggimenti dell'halfling).~ }
\par \pard\plain \ltrpar\s1\rtlch\afs24\lang255\ltrch\dbch\langfe1040\hich\fs24\lang1040\loch\fs24\lang1040 {\rtlch \ltrch\loch\f1\fs20\lang1040\i0\b0 == JAHEIRJ IF ~IsValidForPartyDialog("Jaheira") !StateCheck("Jaheira",CD_STATE_NOTVALID)~ THEN ~Vorresti portare quest'uomo di fronte alla giustizia? In assenza di prove non credo che andremmo molto lontani. Sarebbe bene rivolgersi alle autorit\'e0 locali. Un
a collaborazione potrebbe risultare fruttuosa.~}
\par \pard\plain \ltrpar\s1\rtlch\afs24\lang255\ltrch\dbch\langfe1040\hich\fs24\lang1040\loch\fs24\lang1040 {\rtlch \ltrch\loch\f1\fs20\lang1040\i0\b0 == CERNDJ IF ~IsValidForPartyDialog("Cernd") !StateCheck("Cernd",CD_STATE_NOTVALID)~ THEN ~Come un ragno tesse abilmente la propria tela a spese degli ignari insetti, cos\'ec quest'uomo ha costruito la sua fortuna sulle disgrazie altrui. E' nostro dovere inte
rvenire affinch\'e8 la trama della sua ragnatela non si espanda ulteriormente.~}
\par \pard\plain \ltrpar\s1\rtlch\afs24\lang255\ltrch\dbch\langfe1040\hich\fs24\lang1040\loch\fs24\lang1040 {\rtlch \ltrch\loch\f1\fs20\lang1040\i0\b0 == VALYGAJ IF ~IsValidForPartyDialog("Valygar") !StateCheck("Valygar",CD_STATE_NOTVALID)~ THEN ~Pare che il novero dei criminali abbia accolto al suo interno anche i mercanti, come se l\'92attuale numero non fosse gi\'e0 abbastanza. Forse questa \'e8 l\'92occasione pe
r mostrar loro che ad Athkatla nessuno \'e8 libero di agire come meglio crede a spese degli altri.~}
\par \pard\plain \ltrpar\s1\rtlch\afs24\lang255\ltrch\dbch\langfe1040\hich\fs24\lang1040\loch\fs24\lang1040 {\rtlch \ltrch\loch\f1\fs20\lang1040\i0\b0 == KORGANJ IF ~IsValidForPartyDialog("Korgan") !StateCheck("Korgan",CD_STATE_NOTVALID)~ THEN ~Questo Saerk \'e8 schifosamente simile ai miei compagni d'un tempo. Per i Nove Inferi, io dico di ucciderlo e di prenderci tutte le sue ricchezze!~}
\par \pard\plain \ltrpar\s1\ql\rtlch\afs20\lang255\ltrch\dbch\langfe1040\hich\fs20\lang1033\loch\fs20\lang1033 {\rtlch \ltrch\loch\f1\fs20\lang1033\i0\b0 == AERIEJ IF ~IsValidForPartyDialog("Aerie") !StateCheck("Aerie",CD_STATE_NOTVALID)~ THEN}
\par \pard\plain \ltrpar\s1\ql\rtlch\afs20\lang255\ltrch\dbch\langfe1040\hich\fs20\lang1040\loch\fs20\lang1040 {\rtlch \ltrch\loch\f1\fs20\lang1040\i0\b0 ~E'... E' molto triste quello che ti \'e8 successo, Stivan. Io... Non pensavo che certi uomini potessero essere cos\'ec cattivi.~}
\par \pard\plain \ltrpar\s1\ql\rtlch\afs24\lang255\ltrch\dbch\langfe1040\hich\fs20\lang1040\loch\fs20\lang1040 {\rtlch \ltrch\loch\f1\fs20\lang1040\i0\b0 == HAERDAJ IF ~IsValidForPartyDialog("Haerdali") !StateCheck("Haerdali",CD_STATE_NOTVALID)~ THEN ~Ah, un colpo di scena! Il mandante dell'assassinio della giovane donna si rivela essere anche l'artefice della miseria del mio piccolo gabbiano. Quale sar\'e0 il
 prossimo sviluppo in questa tragica vicenda?~}
\par \pard\plain \ltrpar\s1\ql\rtlch\afs20\lang255\ltrch\dbch\langfe1040\hich\fs20\lang1040\loch\fs20\lang1040 {\rtlch \ltrch\loch\f1\fs20\lang1040\i0\b0 == NALIAJ IF ~IsValidForPartyDialog("Nalia") !StateCheck("Nalia",CD_STATE_NOTVALID)~ THEN}
\par \pard\plain \ltrpar\s1\ql\rtlch\afs24\lang255\ltrch\dbch\langfe1040\hich\fs20\lang1040\loch\fs20\lang1040 {\rtlch \ltrch\loch\f1\fs20\lang1040\i0\b0 ~Sono basita. Non ho mai visto simili soprusi neanche tra le pi\'f9 altolocate famiglie di Athkatla. E a che scopo? Mira forse ad una scalata sociale?~}
\par \pard\plain \ltrpar\s1\rtlch\afs24\lang255\ltrch\dbch\langfe1040\hich\fs24\lang1040\loch\fs24\lang1040 {\rtlch \ltrch\loch\f1\fs20\lang1040\i0\b0 = ~Non importa quali siano le sue motivazioni. Dobbiamo fare il possibile per restituire alle vittime di questo mercante ci\'f2 che \'e8 stato loro sottratto!~}
\par \pard\plain \ltrpar\s1\rtlch\afs24\lang255\ltrch\dbch\langfe1040\hich\fs24\lang1040\loch\fs24\lang1040 {\rtlch \ltrch\loch\f1\fs20\lang1040\i0\b0 == VICONIJ IF ~IsValidForPartyDialog("Viconia") !StateCheck("Viconia",CD_STATE_NOTVALID)~ THEN ~Il rivvil merita di essere ripagato con la stessa moneta, sebbene non si possa negare che abbia un certo talento nel dimostrare la propria superiorit\'e0 sui debol
i.~}
\par \pard\plain \ltrpar\s1\rtlch\afs24\lang255\ltrch\dbch\langfe1040\hich\fs24\lang1040\loch\fs24\lang1040 {\rtlch \ltrch\loch\f1\fs20\lang1040\i0\b0 == YOSHJ IF ~IsValidForPartyDialog("Yoshimo") !StateCheck("Yoshimo",CD_STATE_NOTVALID)~ THEN ~Saerk Farrahd \'e8 un uomo molto potente, e il suo seguito di guardie \'e8 noto per essere poco\'85 amichevole. Se siamo destinati ad inimicarcelo, dovremmo agire con caut
ela.~}
\par \pard\plain \ltrpar\s1\rtlch\afs24\lang255\ltrch\dbch\langfe1040\hich\fs24\lang1040\loch\fs24\lang1040 {\rtlch \ltrch\loch\f1\fs20\lang1040\i0\b0 == IMOEN2J IF ~IsValidForPartyDialog("Imoen2") !StateCheck("Imoen2",CD_STATE_NOTVALID)~ THEN ~<CHARNAME> ed io abbiamo gi\'e0 incontrato simili sbruffoni in passato, ma questo pallone gonfiato li supera tutti. Propongo una settimana rieducativa nei sotterrane
i di Candlekeep. Sono sicura che gli farebbe abbassare la cresta, oh s\'ec.~}
\par \pard\plain \ltrpar\s1\rtlch\afs24\lang255\ltrch\dbch\langfe1040\hich\fs24\lang1040\loch\fs24\lang1040 {\rtlch \ltrch\loch\f1\fs20\lang1040\i0\b0 == MAZZYJ IF ~IsValidForPartyDialog("Mazzy") !StateCheck("Mazzy",CD_STATE_NOTVALID)~ THEN ~Dobbiamo intervenire in difesa della giustizia, ma con i mezzi appropriati. Un processo di fronte ad una corte \'e8 ci\'f2 che quell'individuo merita!~}
\par \pard\plain \ltrpar\s1\rtlch\afs24\lang255\ltrch\dbch\langfe1040\hich\fs24\lang1040\loch\fs24\lang1040 {\rtlch \ltrch\loch\f1\fs20\lang1040\i0\b0 == JANJ IF ~IsValidForPartyDialog("Jan") !StateCheck("Jan",CD_STATE_NOTVALID)~ THEN ~Per la barba di zio Scratchy! I mercanti sono per definizione dei truffatori, ma il Calimshita non conosce scrupoli! Neanche in occasione della Grande Carestia di Rape del
 1348 fui cos\'ec disonesto. Mi limitai a spacciare per rape delle zucchine adeguatamente incantate con un banale incantesimo d'illusione. Essendo il solo a commerciarle, feci soldi a palate, ma questo attir\'f2 l'attenzione dell'esattore delle Tasse e delle Ent
rate di Amn, un tale di nome Trax. Sai, proprio poco tempo fa stava per mettermi nel sacco, ma sono riuscita a cavarmela grazie al provvidenziale intervento di un gruppo di avventurieri. E' un peccato non averli pi\'f9 rivisti.~ }
\par \pard\plain \ltrpar\s1\rtlch\afs24\lang255\ltrch\dbch\langfe1040\hich\fs24\lang1040\loch\fs24\lang1040 {\rtlch \ltrch\loch\f1\fs20\lang1040\i0\b0 == KELDORNJ IF ~IsValidForPartyDialog("Keldorn") !StateCheck("Keldorn",CD_STATE_NOTVALID)~ THEN ~Il racconto del ladro dissolve ogni dubbio sulla malvagit\'e0 di quest'uomo, ma rimane il fatto che non vi sono prove della sua colpevolezza n\'e8 in uno n\'e8 nell'alt
ro caso. No, la questione va risolta affidandoci al giudizio di Torm.~}
\par \pard\plain \ltrpar\s1\rtlch\afs24\lang255\ltrch\dbch\langfe1040\hich\fs24\lang1040\loch\fs24\lang1040 {\rtlch \ltrch\loch\f1\fs20\lang1040\i0\b0 == EDWINJ IF ~IsValidForPartyDialog("Edwin") !StateCheck("Edwin",CD_STATE_NOTVALID)~ THEN ~Dovete rimuginare sulla questione ancora per molto? Non \'e8 necessario avere una mente geniale come la mia per capire che quel mercante ve la far\'e0 ancora una volta a m
eno che non lo uccidiate!~}
\par \pard\plain \ltrpar\s1\rtlch\afs24\lang255\ltrch\dbch\langfe1040\hich\fs24\lang1040\loch\fs24\lang1040 {\rtlch \ltrch\loch\f1\fs20\lang1040\i0\b0 == ANOMENJ IF ~I trascorsi dell'halfing non fanno altro che complicare la situazione, pertanto suggerisco di accantonarli, per il momento.~}
\par \pard\plain \ltrpar\s1\rtlch\afs24\lang255\ltrch\dbch\langfe1040\hich\fs24\lang1040\loch\fs24\lang1040 {\rtlch \ltrch\loch\f1\fs20\lang1040\i0\b0 = ~Mi trovo di fronte ad un bivio, ma non so quale sentiero percorrere. Vendicare la morte di Moira \'e8 contro i dettami dell'Ordine; non vendicarla significherebbe invece non onorare la memoria di mia sorella.~}
\par \pard\plain \ltrpar\s1\rtlch\afs24\lang255\ltrch\dbch\langfe1040\hich\fs24\lang1040\loch\fs24\lang1040 {\rtlch \ltrch\loch\f1\fs20\lang1040\i0\b0 = ~Cosa mi suggerisci di fare, <CHARNAME>?~}
\par \pard\plain \ltrpar\s1\rtlch\afs24\lang255\ltrch\dbch\langfe1040\hich\fs24\lang1040\loch\fs24\lang1040 {\rtlch \ltrch\loch\f1\fs20\lang1040\i0\b0 Tuo padre ha ragione... L'omicidio di tua sorella non pu\'f2 restare impunito. Saerk \'e8 chiaramente malvagio, e non \'e8 forse responsabilit\'e0 di ogni cavaliere distruggere il male?}
\par \pard\plain \ltrpar\s1\rtlch\afs24\lang255\ltrch\dbch\langfe1040\hich\fs24\lang1040\loch\fs24\lang1040 {\rtlch \ltrch\loch\f1\fs20\lang1040\i0\b0 Non lasciare che questi pensieri di vendetta oscurino il tuo giudizio. Uccidere per vendetta \'e8 omicidio. Saerk deve essere portato alla giustizia, e tu sei colui che deve assicurarsi che venga fatto.}
\par \pard\plain \ltrpar\s1\rtlch\afs24\lang255\ltrch\dbch\langfe1040\hich\fs24\lang1040\loch\fs24\lang1040 {\rtlch \ltrch\loch\f1\fs20\lang1040\i0\b0 La risposta \'e8 ovvia. Tutto ci\'f2 che c'\'e8 di male in questa situazione \'e8 il prodotto dei piani di Saerk... sia la povert\'e0 di tuo padre che la morte di tua sorella. Saerk pagher\'e0 con la vita per quella di tua sorella, e la sua ricchezza diventer\'e0 quella di tuo
 padre.}
\par \pard\plain \ltrpar\s1\ql\rtlch\afs24\lang255\ltrch\dbch\langfe1040\hich\fs24\lang1040\loch\fs24\lang1040 
\par \pard\plain \ltrpar\s1\rtlch\afs24\lang255\ltrch\dbch\langfe1040\hich\fs24\lang1040\loch\fs24\lang1040 
\par \pard\plain \ltrpar\s1\ql\rtlch\afs20\lang255\ltrch\dbch\langfe1040\hich\fs20\lang1040\loch\fs20\lang1040 {\rtlch \ltrch\loch\f1\fs20\lang1040\i0\b0 COR Hai visto le ceneri senza vita di tua sorella, Anomen. Non lasciare impunito quest'atto orribile! Lascia che il tuo cuore si riempia di giusta rabbia ed uccidi l'assassino di tua sorella!}
\par \pard\plain \ltrpar\s1\rtlch\afs24\lang255\ltrch\dbch\langfe1040\hich\fs24\lang1040\loch\fs24\lang1040 {\rtlch \ltrch\loch\f1\fs20\lang1040\i0\b0 ANOMEN Giammai, padre. E' tempo che questa follia abbia termine.}
\par \pard\plain \ltrpar\s1\ql\rtlch\afs24\lang255\ltrch\dbch\langfe1040\hich\fs20\lang1040\loch\fs20\lang1040 {\rtlch \ltrch\loch\f1\fs20\lang1040\i0\b0 COR Che cosa intendi dire, Anomen?}
\par \pard\plain \ltrpar\s1\rtlch\afs24\lang255\ltrch\dbch\langfe1040\hich\fs24\lang1040\loch\fs24\lang1040 {\rtlch \ltrch\loch\f1\fs20\lang1040\i0\b0 ANOMEN Solo che uccidere Saerk per vendetta sarebbe omicidio cos\'ec come lo \'e8 stato la morte di mia sorella.}
\par \pard\plain \ltrpar\s1\rtlch\afs24\lang255\ltrch\dbch\langfe1040\hich\fs24\lang1040\loch\fs24\lang1040 {\rtlch \ltrch\loch\f1\fs20\lang1040\i0\b0 COR La distruzione del male non \'e8 mai considerata un omicidio. Quei cavalieri cui cerchi di unirti non compiono forse questo tipo di missioni?}
\par \pard\plain \ltrpar\s1\rtlch\afs24\lang255\ltrch\dbch\langfe1040\hich\fs24\lang1040\loch\fs24\lang1040 {\rtlch \ltrch\loch\f1\fs20\lang1040\i0\b0 ANOMEN Non nel modo che suggerisci. Dobbiamo portare queste prove al magistrato. E' l'unico modo per terminare questo circolo di violenza nel quale ci hai intrappolati.}
\par \pard\plain \ltrpar\s1\ql\rtlch\afs20\lang255\ltrch\dbch\langfe1040\hich\fs20\lang1033\loch\fs20\lang1033 {\rtlch \ltrch\loch\f1\fs20\lang1033\i0\b0 INTERJEC\-\-T_COPY_TRANS ANOMENJ}
\par \pard\plain \ltrpar\s1\ql\rtlch\afs24\lang255\ltrch\dbch\langfe1040\hich\fs20\lang1033\loch\fs20\lang1033 {\rtlch \ltrch\loch\f1\fs20\lang1033\i0\b0 == TB#STIVJ IF ~IsValidForPartyDialog("Stivan") !StateCheck("Stivan",CD_STATE_NOTVALID)~ THEN}
\par \pard\plain \ltrpar\s1\ql\rtlch\afs20\lang255\ltrch\dbch\langfe1040\hich\fs20\lang1040\loch\fs20\lang1040 {\rtlch \ltrch\loch\f1\fs20\lang1040\i0\b0 ~Anomen ha ragione. Non c'\'e8 motivo di sporcarsi le mani di sangue. Il modo migliore per fargliela pagare \'e8 ridurlo in povert\'e0, esattamante come ha fatto con me e con voi.~}
\par \pard\plain \ltrpar\s1\ql\rtlch\afs24\lang255\ltrch\dbch\langfe1040\hich\fs20\lang1040\loch\fs20\lang1040 {\rtlch \ltrch\loch\f1\fs20\lang1040\i0\b0 = ~Quando il magistrato riconoscer\'e0 le sue colpe, perder\'e0 ogni cosa... E l'antico e il nuovo sopruso verranno finalmente vendicati!~}
\par \pard\plain \ltrpar\s1\ql\rtlch\afs20\lang255\ltrch\dbch\langfe1040\hich\fs20\lang1040\loch\fs20\lang1040 {\rtlch \ltrch\loch\f1\fs20\lang1040\i0\b0 COR Stupido ragazzo! Il magistrato non far\'e0 nulla, \'e8 una pedina di Saerk!}
\par \pard\plain \ltrpar\s1\ql\rtlch\afs24\lang255\ltrch\dbch\langfe1040\hich\fs20\lang1040\loch\fs20\lang1040 {\rtlch \ltrch\loch\f1\fs20\lang1040\i0\b0 ANOMEN Bylanna Ianulin \'e8 una donna buona e nobile. L'hai detto tu stesso prima di cadere nell'orribile morsa del bere.}
\par \pard\plain \ltrpar\s1\rtlch\afs24\lang255\ltrch\dbch\langfe1040\hich\fs24\lang1040\loch\fs24\lang1040 {\rtlch \ltrch\loch\f1\fs20\lang1040\i0\b0 COR Tu disonori la memoria di Moira! Consentirai all'assassino di tua sorella di girare libero? Sei ripugnante! Sei un insetto!}
\par \pard\plain \ltrpar\s1\rtlch\afs24\lang255\ltrch\dbch\langfe1040\hich\fs24\lang1040\loch\fs24\lang1040 {\rtlch \ltrch\loch\f1\fs20\lang1040\i0\b0 ANOMEN Non gli permetter\'f2 di rimanere libero! Deve essere condotto davanti alla corte se \'e8 davvero colui che ha assassinato mia sorella.}
\par \pard\plain \ltrpar\s1\rtlch\afs24\lang255\ltrch\dbch\langfe1040\hich\fs24\lang1040\loch\fs24\lang1040 {\rtlch \ltrch\loch\f1\fs20\lang1040\i0\b0 COR Come puoi dubitare di una cosa del genere, ragazzo? Saerk \'e8 l'assassino!}
\par \pard\plain \ltrpar\s1\rtlch\afs24\lang255\ltrch\dbch\langfe1040\hich\fs24\lang1040\loch\fs24\lang1040 {\rtlch \ltrch\loch\f1\fs20\lang1040\i0\b0 ANOMEN Ne hai le prove, padre?}
\par \pard\plain \ltrpar\s1\rtlch\afs24\lang255\ltrch\dbch\langfe1040\hich\fs24\lang1040\loch\fs24\lang1040 {\rtlch \ltrch\loch\f1\fs20\lang1040\i0\b0 COR La prova consiste nel fatto che \'e8 lui ad avermi portato via ogni altra cosa! Uccidere Moira avrebbe completato la mia distruzione.}
\par \pard\plain \ltrpar\s1\rtlch\afs24\lang255\ltrch\dbch\langfe1040\hich\fs24\lang1040\loch\fs24\lang1040 {\rtlch \ltrch\loch\f1\fs20\lang1040\i0\b0 ANOMEN S\'ec, ora capisco. E' sempre stato cos\'ec in questa casa. Riguarda te e solo te. La morte di tua figlia non significa nulla al di l\'e0 di come influisce sul tuo orgoglio e sulla tua tranquillit\'e0.}
\par \pard\plain \ltrpar\s1\rtlch\afs24\lang255\ltrch\dbch\langfe1040\hich\fs24\lang1040\loch\fs24\lang1040 {\rtlch \ltrch\loch\f1\fs20\lang1040\i0\b0 COR Ragazzo, tu stai per cadere preda del male! Allontanati da quel confine e rendi l'onore alla tua famiglia, prima che sia troppo tardi!}
\par \pard\plain \ltrpar\s1\rtlch\afs24\lang255\ltrch\dbch\langfe1040\hich\fs24\lang1040\loch\fs24\lang1040 {\rtlch \ltrch\loch\f1\fs20\lang1040\i0\b0 ANOMEN Non lo far\'f2, padre. Non propongo il male. Propongo il cammino legale.}
\par \pard\plain \ltrpar\s1\rtlch\afs24\lang255\ltrch\dbch\langfe1040\hich\fs24\lang1040\loch\fs24\lang1040 {\rtlch \ltrch\loch\f1\fs20\lang1040\i0\b0 COR Lo dico di nuovo, obbediscimi, Anomen!}
\par \pard\plain \ltrpar\s1\rtlch\afs24\lang255\ltrch\dbch\langfe1040\hich\fs24\lang1040\loch\fs24\lang1040 {\rtlch \ltrch\loch\f1\fs20\lang1040\i0\b0 ANOMEN Ti ho obbedito per tutta la vita e non ho ricevuto altro che amarezza in cambio. Io e i miei amici porteremo questa faccenda davanti al magistrato, come richiede la legge.}
\par \pard\plain \ltrpar\s1\rtlch\afs24\lang255\ltrch\dbch\langfe1040\hich\fs24\lang1040\loch\fs24\lang1040 {\rtlch \ltrch\loch\f1\fs20\lang1040\i0\b0 COR Se esci da quella porta sappi che non dovrai mai pi\'f9 tornare.}
\par \pard\plain \ltrpar\s1\rtlch\afs24\lang255\ltrch\dbch\langfe1040\hich\fs24\lang1040\loch\fs24\lang1040 {\rtlch \ltrch\loch\f1\fs20\lang1040\i0\b0 ANOMEN Non farlo, padre...}
\par \pard\plain \ltrpar\s1\rtlch\afs24\lang255\ltrch\dbch\langfe1040\hich\fs24\lang1040\loch\fs24\lang1040 {\rtlch \ltrch\loch\f1\fs20\lang1040\i0\b0 COR Taci! Se te ne vai ora non potrai tornare mai pi\'f9. Sarai cacciato dalla famiglia e diventerai un cane bastardo, indegno di rannicchiarsi ai miei piedi.}
\par \pard\plain \ltrpar\s1\rtlch\afs24\lang255\ltrch\dbch\langfe1040\hich\fs24\lang1040\loch\fs24\lang1040 {\rtlch \ltrch\loch\f1\fs20\lang1040\i0\b0 ANOMEN Mi sono rannicchiato ai tuoi piedi per tutta la vita. Addio, padre. Forse ti rivedr\'f2 ancora prima che tu beva fino ad ucciderti.}
\par \pard\plain \ltrpar\s1\rtlch\afs24\lang255\ltrch\dbch\langfe1040\hich\fs24\lang1040\loch\fs24\lang1040 {\rtlch \ltrch\loch\f1\fs20\lang1040\i0\b0 COR Tu non sei nulla, ragazzo! Nulla!}
\par \pard\plain \ltrpar\s1\rtlch\afs24\lang255\ltrch\dbch\langfe1040\hich\fs24\lang1040\loch\fs24\lang1040 {\rtlch \ltrch\loch\f1\fs20\lang1040\i0\b0 ANOMEN Vieni, <CHARNAME>, andiamocene... Dovremmo andare a far visita al magistrato nel palazzo del Consiglio dei Sei e vediamo se si pu\'f2 fare qualcosa, nonostante la cattiveria del padre.}
\par \pard\plain \ltrpar\s1\ql\rtlch\afs20\lang255\ltrch\dbch\langfe1040\hich\fs20\lang1033\loch\fs20\lang1033 {\rtlch \ltrch\loch\f1\fs20\lang1033\i0\b0 INTERJEC\-\-T_COPY_TRANS ANOMENJ}
